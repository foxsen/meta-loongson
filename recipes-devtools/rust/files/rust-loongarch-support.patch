+/// linker flavors (`LinkerFlavor`).
+#[derive(Clone, Copy, Debug, Eq, Ord, PartialEq, PartialOrd)]
+pub enum LinkerFlavorCli {
+    Gcc,
+    Ld,
+    Lld(LldFlavor),
+    Msvc,
+    Em,
+    BpfLinker,
+    PtxLinker,
+}
