+    fn debuginfo_map_to(&self, which: GitRepo) -> Option<String> {
+        if !self.config.rust_remap_debuginfo {
+            return None;
+        }
+
+        match which {
+            GitRepo::Rustc => {
+                let sha = self.rust_sha().unwrap_or(&self.version);
+                Some(format!("/rustc/{}", sha))
+            }
+            GitRepo::Llvm => Some(String::from("/rustc/llvm")),
