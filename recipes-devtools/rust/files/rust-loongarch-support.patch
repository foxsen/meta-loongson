+                            ANONYMOUS_PARAMETERS,
+                            arg.pat.span,
+                            fluent::lint_builtin_anonymous_params,
+                            |lint| {
+                                lint.span_suggestion(
+                                    arg.pat.span,
+                                    fluent::suggestion,
+                                    format!("_: {}", ty_snip),
+                                    appl,
+                                )
+                            },
+                        )
+                    }
+                }
+            }
+        }
+    }
