+#[derive(Copy, Clone, PartialEq, Eq, PartialOrd, Ord, Debug)]
+pub enum Kind {
+    Build,
+    Check,
+    Clippy,
+    Fix,
+    Format,
+    Test,
+    Bench,
+    Doc,
+    Clean,
+    Dist,
+    Install,
+    Run,
+    Setup,
+}
+
+impl Kind {
+    pub fn parse(string: &str) -> Option<Kind> {
+        // these strings, including the one-letter aliases, must match the x.py help text
+        Some(match string {
+            "build" | "b" => Kind::Build,
+            "check" | "c" => Kind::Check,
+            "clippy" => Kind::Clippy,
+            "fix" => Kind::Fix,
+            "fmt" => Kind::Format,
+            "test" | "t" => Kind::Test,
+            "bench" => Kind::Bench,
